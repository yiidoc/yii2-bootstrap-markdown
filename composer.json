{
    "name": "yiidoc/yii2-bootstrap-markdown",
    "description": "Extension bootstrap markdown for Yii2 Framework.",
    "keywords": [
        "yii2",
        "extension",
        "bootstrap",
        "WYSIWYG",
        "markdown editor"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiidoc/yii2-bootstrap-markdown/issues?state=open",
        "source": "https://github.com/yiidoc/yii2-bootstrap-markdown"
    },
    "authors": [
        {
            "name": "Nghia Nguyen",
            "email": "zinzinday@gmail.com"
        }
    ],
    "autoload": {
